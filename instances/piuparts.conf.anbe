[experimental/main]
precedence = 40
piuparts-flags =
	%(flags-default)s
depends-sections = sid/main
distro = experimental
arch = amd64
area = main
components = main
reschedule-old-days = 30

[experimental/contrib]
precedence = 45
piuparts-flags =
	%(flags-default)s
depends-sections = experimental/main sid/main sid/contrib sid/non-free
